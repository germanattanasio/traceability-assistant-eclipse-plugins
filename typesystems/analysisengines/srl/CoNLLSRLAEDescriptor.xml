<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>edu.isistan.uima.unified.analysisengines.srl.CoNLLSRLAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>CoNLLSRLAEDescriptor</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>model</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>propbank</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>nombank</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>model</name>
        <value>
          <string><envVarRef>MODELS_PATH</envVarRef>srl/se/lth/cs/srl/model/srl-eng.model</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>propbank</name>
        <value>
          <string><envVarRef>MODELS_PATH</envVarRef>srl/propbank/</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>nombank</name>
        <value>
          <string><envVarRef>MODELS_PATH</envVarRef>srl/nombank/</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="../../typesystems/nlp/TokenTypeDescriptor.xml"/>
        <import location="../../typesystems/nlp/CoNLLDependencyTypeDescriptor.xml"/>
        <import location="../../typesystems/srl/PredicateTypeDescriptor.xml"/>
        <import location="../../typesystems/srl/ArgumentTypeDescriptor.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
